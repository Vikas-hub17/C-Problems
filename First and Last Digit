#include<bits/stdc++.h>

using namespace std;
#define el endl;

void FIRST_LAST(int n){
    int l;
    l=n%10;
    while(n>9){
    n=n/10;
    }
    int sum=n+l;
    cout<<sum<<endl;
}

int main()
{
  int t,n;
  cin>>t;
  for(int i=0;i<t;i++){
      cin>>n;
      FIRST_LAST(n);
  }
return 0;

}
