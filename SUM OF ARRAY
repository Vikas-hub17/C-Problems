#include <iostream>
#include <algorithm>
using namespace std;

void SUM_array(int array[], int n){
    int sum=0;
    for(int i=0;i<n;i++){
        sum=sum+array[i];
    }
    cout<<"SUM of all elements: " << sum<<endl;
}


int main(){
    int size;
    cin>>size;
    int array[1000];
    for(int i=0;i<size;i++){
        cin>>array[i];
    }
    SUM_array(array, size);
}
