#include <algorithm>
using namespace std;

int linear_search(int array[], int key){
    for(int i=0;i<5;i++){
        if(array[i]==key){
            cout<<"Key is present on index:" << i <<endl;
        }
    }
   cout<<"Not found";
}

int main(){
    
    
    int array[5];
    cout<<"ENTER ELEMENTS OF ARRAY:";
    for(int i=0;i<5;i++){
        cin>>array[i];
    }
    cout<<"Enter the key:";
    int key;
    cin>>key;
    linear_search(array, key);
    return 0;
}
