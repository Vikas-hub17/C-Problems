#include <iostream>
using namespace std;

int main() {
	int t,D,L,R;
	cin >> t;
	for(int i=0;i<t;i++){
	    cin>>D>>L>>R;
	    if(D>=L && D<=R){
	        cout<< "Take second dose now"<<endl;
	    }
	    else if(D>L && D>R){
	        cout<<"Too Late"<<endl;
	    }
	    else if(D<L && D<R){
	        cout<<"Too Early"<<endl;
	    }
	    else
	    cout<<"Take second dose now"<<endl;
	}
	return 0;
}
